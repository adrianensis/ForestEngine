{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "forest build debug",
			"command": "python3",
			"args": [
                "-B",
				"./scripts/build.py",
				"-x",
				"-i",
				"-l"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "forest build release",
			"command": "python3",
			"args": [
                "-B",
				"./scripts/build.py",
				"-x",
				"-i",
				"-l",
				"-r"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "shell",
			"label": "forest clean",
			"command": "./scripts/clean.sh",
			"group": "build"
		}
	]
